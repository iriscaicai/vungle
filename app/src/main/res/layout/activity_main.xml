<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <Button
        android:id="@+id/button_load_inter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/load_insert"
        android:visibility="visible" />

    <Button
        android:id="@+id/button_show_ad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/show_insert"
        android:visibility="gone" />

    <Button
        android:id="@+id/button_load_infeed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/load_infeed"
        android:visibility="visible" />

    <Button
        android:id="@+id/button_show_infeed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/show_infeet"
        android:visibility="gone" />

    <RelativeLayout
        android:id="@+id/infeed_container"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:background="#aabbcc"></RelativeLayout>
</LinearLayout>
